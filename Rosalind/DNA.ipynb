   "source": [
    "with open('rosalind_dna.txt', 'r') as f:\n",
    "    dna=f.read()\n",
    "countA= 0\n",
    "countG=0\n",
    "countC=0\n",
    "countT=0\n",
    "for i in dna:\n",
    "    if i== \"A\":\n",
    "        countA +=1\n",
    "    if i==\"G\":\n",
    "        countG += 1\n",
    "    if i==\"C\":\n",
    "        countC +=1 \n",
    "    if i==\"T\":\n",
    "        countT += 1\n",
    "print(countA,countC,countG,countT)"
   ]
  }
