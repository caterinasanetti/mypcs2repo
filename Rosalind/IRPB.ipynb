   "source": [
    "import re\n",
    "with open('rosalind_iprb.txt', 'r') as f:\n",
    "    s=f.read()\n",
    "x = re.findall('[0-9]+', s)\n",
    "a=[]\n",
    "for i in x:\n",
    "    a.append(int(i))\n",
    "k=a[0]\n",
    "m=a[1]\n",
    "n=a[2]\n",
    "nres=(k*(k-1)+(3/4)*(m*(m-1))+2*(k*m)+2*(k*n)+(m*n))\n",
    "dres=((k+m+n)*(k+m+n-1))\n",
    "res=float(nres/dres)\n",
    "print(res)"
   ]
  }
