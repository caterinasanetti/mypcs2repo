    "from math import perm\n",
    "import re\n",
    "with open('rosalind_pper.txt', 'r') as f:\n",
    "    s=f.read()\n",
    "x = re.findall('[0-9]+', s)\n",
    "s=[]\n",
    "for i in x:\n",
    "    s.append(int(i))\n",
    "a=s[0]\n",
    "b=s[1]\n",
    "pperm=perm(a,b)\n",
    "print(pperm%1000000)"
   ]
  }
