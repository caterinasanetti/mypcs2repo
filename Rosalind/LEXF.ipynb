   "source": [
    "from itertools import product\n",
    "with open('rosalind_lexf.txt', 'r') as f:\n",
    "    n= f.readline().split()\n",
    "    x=(f.readline().strip())\n",
    "y=int(x)\n",
    "l =list(product(n,repeat =y))\n",
    "for item in l:\n",
    "    n=\"\".join(map(str,item))\n",
    "    print(n)"
   ]
  }
